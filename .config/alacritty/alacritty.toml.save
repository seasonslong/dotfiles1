:qqXX[general]
import = ["~/.config/alacritty/colors.toml"]

[window]
padding = { x = 15, y = 15 }

[font]
size = 12.0

[font.bold]
family = "IosevkaTerm NFM"
style = "Bold"

[font.bold_italic]
family = "IosevkaTerm NFM"
style = "Bold Italic"

[font.italic]
family = "IosevkaTerm NFM"
style = "Italic"

[font.normal]
family = "IosevkaTerm NFM"
style = "Regular"

[keyboard]
bindings = [
   { key = "Return", mods = "Control|Shift", action = "SpawnNewInstance" }
]

[env]
TERM = "xterm-256color"
